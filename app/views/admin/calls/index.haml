%table.table
  %thead
    %th
      \#
    %th
      Имя
    %th
      Телефон
    %th
      Дата заявки
    %th
      Тип
    %th
      Actions
  %tbody
    - @calls.each do |call|
      %tr
        %td
          = call.id
        %td
          = call.username
        %td
          = call.userphone
        %td
          = call.created_at
        %td
          = call.call_type
        %td
          = link_to 'Show',  admin_call_path(call)