.col-md-4
  = form_for [:admin, @comp], :html => { :multipart => true } do |f|
    .form-group
      =f.label 'Изображение', class: 'control-label'
      = file_field_tag 'imgs[]', type: :file, multiple: true, class: 'form-control'
    .form-group
      =f.submit 'Создать композицию', class: 'btn btn-primary'
