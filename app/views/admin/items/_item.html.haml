- @items.each do |item|
  %p{style: 'float: left; width: 50%; min-height: 100px;'}
    %br
    = image_tag item.img.url(:small)
    = link_to item.name, admin_item_path(item)
    %br
    Минимальная партия:
    = item.min_order
    = item.quantity_type
%div{styl: 'clear:both'}
= will_paginate @items
